a = 6
b = 7
c = (a+b)/2

voornaam = 'Thom'
tussenvoegsel = ''
achternaam = 'Winterswijk'
mijnnaam = voornaam + ' ' + tussenvoegsel + ' ' + achternaam

print(b < 75 > a)
print(len(mijnnaam) == len(voornaam+tussenvoegsel+achternaam))
print(len(tussenvoegsel) *5 >= len(mijnnaam))
print(tussenvoegsel in achternaam)
